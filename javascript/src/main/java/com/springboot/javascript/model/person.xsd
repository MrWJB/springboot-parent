<?xml version="1.0" encoding="UTF-8" ?>
<xs:element name="person" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:complexType>
        <xs:sequence>
            <xs:element type="xs:string" name="addr" minOccurs="0"></xs:element>
            <xs:element type="xs:int" name="age"></xs:element>
            <xs:element type="xs:date" name="date"></xs:element>
            <xs:element type="xs:string" name="name"></xs:element>
            <xs:sequence>
                <xs:element type="xs:string" name="key"></xs:element>
            </xs:sequence>
        </xs:sequence>
        <xs:attribute type="xs:string" name="gender"></xs:attribute>
    </xs:complexType>
</xs:element>